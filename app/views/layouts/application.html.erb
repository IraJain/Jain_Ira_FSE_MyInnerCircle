<!DOCTYPE html>
<html>
<head>
  <title>CircleTest</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

  <%= render 'layouts/iesupport' %>

</head>
<body>

<% if user_signed_in? %>
<%= render 'layouts/head_user' %>
<% end %>

<% if !flash.empty? %>
<div id= "user_err">
<%= render "layouts/error" %>
</div>
<% end %>

<div class="container">
<%= yield %>
</div>

</body>
</html>
